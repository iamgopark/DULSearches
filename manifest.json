{
   "name": "Duke Library Searches",
   "description": "This extension will find books and articles at Duke Libraries, public libraries in the area, and Amazon with a right-click.",
   "version": "1.0",

   "author" : "Sarah Park",
   "permissions": ["tabs",  
      "contextMenus"
   ],

   "background": {
      "service_worker": "background.js"
   },

   "icons": {
      "16": "/images/plugin16.png",
      "32": "/images/plugin32.png",
      "48": "/images/plugin48.png",
      "128": "/images/plugin128.png"
   },

   "action": {
      "default_icon": {
         "16": "/images/plugin16.png",
         "32": "/images/plugin32.png",
         "48": "/images/plugin48.png",
         "128": "/images/plugin128.png"
     }
   },

   "update_url": "https://clients2.google.com/service/update2/crx",
   "manifest_version": 3
}
